{
  "expo": {
    "name": "Gimme",
    "slug": "gimme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "A virtual marketplace app built with React Native. I used this project to get acquainted with jsx for a larger passion project I'm currently researching and user testing for.",
    "githubUrl": "https://github.com/allyson09/React-Native-Tutorial"
  }
}
